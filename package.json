{
  "name": "lerna-semantic-release",
  "description": "automated semver compliant package publishing, for lerna",
  "author": "Joshua Nelson <jonelson@atlassian.com>, Joscha Feth <jfeth@atlassian.com>",
  "bin": {
    "lerna-semantic-release": "bin/lerna-semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/atlassian/lerna-semantic-release/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog/"
    }
  },
  "dependencies": {
    "@semantic-release/commit-analyzer": "2.0.0",
    "@semantic-release/last-release-npm": "1.2.1",
    "async": "1.5.2",
    "npmconf": "2.1.2",
    "rc": "1.1.6",
    "semantic-release": "4.3.5",
    "semantic-release-lerna-analyzer": "0.0.1",
    "shelljs": "0.7.0"
  },
  "peerDependencies": {
    "lerna": "*"
  },
  "devDependencies": {
    "cz-conventional-changelog": "1.1.6",
    "lerna": "jpnelson/lerna#8c0e5a44",
    "semantic-release": "^4.3.5"
  },
  "engines": {
    "node": ">=0.10",
    "npm": ">=2"
  },
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version",
    "lerna"
  ],
  "license": "MIT",
  "main": "bin/semantic-release.js",
  "files": [
    "bin",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian/lerna-semantic-release.git"
  },
  "scripts": {
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "analyzeCommits": "semantic-release-lerna-analyzer",
    "verifyConditions": [],
    "debug": false
  },
  "version": "0.4.3"
}
