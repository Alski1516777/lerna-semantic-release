{
  "name": "lerna-semantic-release-main",
  "description": "automated semver compliant package publishing, for lerna",
  "author": "Joshua Nelson <jonelson@atlassian.com>, Joscha Feth <jfeth@atlassian.com>",
  "bin": {
    "lerna-semantic-release": "bin/lerna-semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/atlassian/lerna-semantic-release/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog/"
    }
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version",
    "lerna"
  ],
  "license": "MIT",
  "devDependencies": {
    "lerna-semantic-release": "^0.4.0",
    "lerna": "2.0.0-beta.20",
    "cz-conventional-changelog": "~1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian/lerna-semantic-release.git"
  },
  "scripts": {
    "semantic-release": "lerna-semantic-release pre && lerna-semantic-release-perform && lerna-semantic-release post",
    "test": "lerna run test"
  },
  "release": {
    "branch": "caribou"
  },
  "version": "0.0.0"
}
