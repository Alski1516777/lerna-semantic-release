{
  "name": "lerna-semantic-release",
  "description": "automated semver compliant package publishing, for lerna",
  "version": "0.0.1",
  "author": "Joshua Nelson <jonelson@atlassian.com>",
  "bin": {
    "lerna-semantic-release": "bin/lerna-semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/atlassian/lerna-semantic-release/issues"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog/"
  },
  "dependencies": {
    "@semantic-release/last-release-npm": "1.2.1",
    "async": "1.5.2",
    "lerna": "jpnelson/lerna#8c0e5a44",
    "nodegit": "0.13.0",
    "npmconf": "2.1.2",
    "rc": "1.1.6",
    "semantic-release": "4.3.5",
    "semantic-release-lerna-analyzer": "0.0.1"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=0.10",
    "npm": ">=2"
  },
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "bin/semantic-release.js",
  "files": [
    "bin",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlassian/lerna-semantic-release.git"
  },
  "scripts": {
    "lerna-semantic-release": "bin/lerna-semantic-release.js"
  },
  "release": {
    "analyzeCommits": "semantic-release-lerna-analyzer",
    "verifyConditions": [],
    "debug": false
  }
}
